\par
        BG.3  \cf1 COP800 INSTRUCTION SET \cf0\par
\par
            Instruction Argument Syntax:\par
        \par
                A       8-Bit Accumulator Register\par
                B       8-Bit Address Register\par
                X       8-Bit Address Register\par
                [B]     Memory Indirect Addressed By B\par
                [X]     Memory Indirect Addressed By X\par
                MD      Direct Addressed Memory\par
                Mem     Direct Addressing or [B]\par
                Memi    Direct Addressing or [B] or Immediate\par
                Imm     8-Bit Immediate Data\par
                Reg     Register Memory: R0-R15, B, X, SP, 0xF0-0xFF\par
                Addr    Location Address\par
                C       Carry Bit\par
                HC      Half Carry Bit\par
\par
        The terms MD, Mem, Memi, Imm, Reg, and Addr may be expressions. \par
\par
           Note  that  not  all addressing modes may be valid with every\par
        instruction.  Refer to  the  COP800  technical  data  for  valid\par
        modes.  \par
\par
           The  following tables list the mnemonics and arguments recog-\par
        nized by the ASCOP8 assembler.  \par
\par
\par
        BG.3.1  COP800 Instructions \par
\par
            Arithmetic and Logic Instructions\par
                add     A,Memi  (A <- A + Memi)\par
                adc     A,Memi  (A <- A + Memi + C)\par
                                (C <- Carry, HC <- Half Cary)\par
                subc    A,Memi  (a <- A - Memi + C)\par
                                (C <- Carry, HC <- Half Cary)\par
                and     A,Memi  (A <- A and /Memi)\par
                or      A,Memi  (A <- A or Memi)\par
                xor     A,Memi  (A <- A xor Memi)\par
                ifeq    A,Memi  (Do Next if A = Memi)\par
                ifgt    A,Memi  (Do next if A > Memi)\par
                ifbne   #       (Do Next if <3:0> of B != Imm)\par
                drsz    Reg     (Reg <- Reg - 1, Skip If Reg = 0)\par
        \par
                setb    #,Mem   (1 to Bit, Mem (Bit = 0 to 7)\par
                rstb    #,Mem   (0 to Bit, Mem (Bit = 0 to 7)\par
                ifbit   #,Mem   (If Bit = 1, Do Next Instruction)\par
\par
\page\par
\par
        ASCOP8 ASSEMBLER                                       PAGE BG-7\par
        COP800 INSTRUCTION SET\par
\par
\par
            Instructions Using A and C\par
                clr     A       (A <- A)\par
                inc     A       (A <- A + 1)\par
                dec     A       (A <- A - 1)\par
                laid            (A <- ROM(PU,A))\par
                dccr    A       (A <- BCD Correction of A)\par
                rrc     A       (Rotate A Right Through C)\par
                swap    A       (<A7:A4> <--> <A3:A0>)\par
                sc              (C <- 1, HC <- 1)\par
                rc              (C <- 0, HC <- 0)\par
                ifc             (If C = 1, Do Next Instruction)\par
                ifnc            (If C = 0, Do Next Instruction)\par
\par
            Transfer of Control Instructions\par
                jmpl    Addr    (Jump Absolute Long <14:00>)\par
                jmp     Addr    (Jump Absolute Short <11:00>)\par
                jp      Addr    (Relative Jump -31 to +32)\par
                jsrl    Addr    (Jump Subroutine Long <14:00>)\par
                jsr     Addr    (Jump Subroutine Short <11:00>)\par
                jid             (Jump Indirect in Current Page)\par
                ret             (Return From Subroutine)\par
                retsk           (Return From Subroutine, Skip)\par
                reti            (Return From Interrupt)\par
                intr            (Generate An Interrupt)\par
                nop             (No Operation)\par
\par
            Memory Transfer Instructions\par
                x       A,Mem   (A <--> Mem)\par
                x       A,[X]   (A <--> [X])\par
                x       A,[B+-] (A <--> [B], B <- B +/- 1)\par
                x       A,[X+-] (A <--> [X], X <- X +/- 1)\par
                ld      A,Memi  (A <- Memi)\par
                ld      A,[X]   (A <- [X])\par
                ld      A,[B+-] (A <- [B], B <- B +/- 1)\par
                ld      A,[X+-] (A <- [X], X <- X +/- 1)\par
                ld      B,Imm   (A <- Imm)\par
                ld      Mem,Imm (Mem <- Imm)\par
                ld      Reg,Imm (Reg <- Imm)\par
                ifeq    MD,Imm  (Do Next Instruction If MD = Imm)\par
\par
\page\par
\par
        ASCOP8 ASSEMBLER                                       PAGE BG-8\par
        COP800 INSTRUCTION SET\par
\par
\par
            Extended Instructions\par
                andsz   A,Imm   (Skip Next If (A and Imm) = 0)\par
                ifeq    MD,Imm  (Do Next If MD = Imm)\par
                ifne    A,Imm   (Do Next If A = Imm)\par
                ld      B,Imm   (A <- Imm, Faster Form)\par
                pop     A       (POP The Stack Into A)\par
                push    A       (PUSH A Onto The Stack)\par
                rlc     A       (Rotate A Left Through C)\par
                rpnd            (Reset Software Interrupt Pending Flag)\par
                vis             (Vector To Software Interrupt Routine)\par
\par
            Instruction Alternates\par
                clra            (A <- 0)\par
                inca            (A <- A + 1)\par
                deca            (A <- A - 1)\par
                dccra           (A <- BCD Correction of A)\par
                popa            (POP The Stack Into A)\par
                pusha           (PUSH A Onto The Stack)\par
                rrca            (Rotate A Right Through C)\par
                rlca            (Rotate A Left Through C)\par
                swapa           (<A7:A4> <--> <A3:A0>)\par
