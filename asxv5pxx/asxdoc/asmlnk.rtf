            Transfer Of Control Instructions\par
                jid             (Jump Indirect)\par
                jmp     addr    (Jump)\par
                jp      addr    (Jump within Page)\par
                jp23    addr    (Jump to Page 2 or 3)\par
                jsrp    addr    (Jump to Subroutine Page 2)\par
                jsr     addr    (Jump to Subroutine)\par
\page\par
\par
        ASCOP4 ASSEMBLER                                       PAGE BF-6\par
        COP400 INSTRUCTION SET\par
\par
\par
                ret             (Return from Subroutine)\par
                retsk           (Return from Subroutine then Skip)\par
                halt            (Halt)\par
                it              (Idle until Timer overflow)\par
        \par
            Memory Reference Instructions\par
                came            (Copy A,RAM to E)\par
                camq            (Copy A,RAM to Q)\par
                camt            (Copy A,RAM to T)\par
                cema            (Copy E to A,RAM)\par
                cqma            (Copy Q to A,RAM)\par
                ctma            (Copy T to A,RAM)\par
                ld      r       (Load RAM into A, XOR Br with r)\par
                ldd     r,d     (Load RAM into A from Direct r,d)\par
                lqid            (Load Q Indirect)\par
                rmb     n       (Reset RAM Bit)\par
                smb     n       (Set RAM Bit)\par
                stii    #y      (Store Immediate to RAM, Increment Bd)\par
                x       r       (RAM <--> A, Br XOR r)\par
