                            216 	.macro	.pcr_lclx A,B
                            217 	  .globl A
                            218 	  .ifb B
                            219 	    x'A == . + 4
                            220 	  .else
                            221 	    x'A == . + B
                            222 	  .endif
                            223 	.endm
                            224 
                            225 	.macro	.pcr_xtrn A,B,C
                            226 	  .globl A
                            227 	  .ifb C
                            228 	    x'A == B + . + 4
                            229 	  .else
                            230 	    x'A == B + . + C
                            231 	  .endif
                            232 	.endm
